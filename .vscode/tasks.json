{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/FanucRelease.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "rebuild",
      "type": "shell",
      "command": "dotnet clean && dotnet build",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$msCompile",
      "windows": {
        "command": "dotnet clean && dotnet build"
      }
    }
  ]
}